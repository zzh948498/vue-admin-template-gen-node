{
  "name": "RuoYi-Vue3-Node",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:staging": "node dist/main",
    "start:prod": "pm2 start dist/main.js -i max",
    "start:local": "pm2 start dist/main.js --name gen-node",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@adminjs/express": "^4.1.3",
    "@adminjs/nestjs": "^4.0.1",
    "@adminjs/typeorm": "^3.0.2",
    "@nestjs/common": "^9.3.1",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.1",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/platform-express": "^9.3.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@zeronejs/cli": "^3.2.2",
    "@zeronejs/utils": "^1.4.2",
    "adminjs": "^5.11.0",
    "axios": "^1.3.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "fs-extra": "^10.1.0",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.0",
    "ts-morph": "^19.0.0",
    "tslib": "^2.5.0",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.9",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.1",
    "@types/express": "^4.17.16",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "28.1.2",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
